{
  "name": "fcal",
  "productName": "fcal",
  "version": "0.0.1",
  "description": "",
  "main": "app/App.js",
  "scripts": {
    "compile": "tsc -p .",
    "start": "electron app/App.js",
    "pack": "electron-builder --dir",
    "dist": "electron-builder --macos --linux --windows --publish always"
  },
  "keywords": [],
  "author": {
    "name": "Santhosh Kumar R",
    "email": "santhoshkumarr1096@gmail.com"
  },
  "license": "ISC",
  "devDependencies": {
    "@types/jquery": "^3.3.31",
    "electron-builder": "^22.8.0",
    "electron": "^7.3.2",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "fcal": "^0.4.2",
    "jquery": "^3.4.1"
  },
  "build": {
    "appId": "com.santhosh.fcal",
    "mac": {
      "category": "public.app-category.utility"
    },
    "dmg": {
      "iconSize": 160,
      "icon": "app/build/icons/icon.png",
      "contents": [
        {
          "x": 180,
          "y": 170
        },
        {
          "x": 480,
          "y": 170,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "icon": "app/build/icons/icon.png",
      "target": [
        "deb"
      ],
      "category": "Utility"
    },
    "win": {
      "icon": "app/build/icons/icon.png"
    }
  }
}